package org.DecisionMaker;

import javax.enterprise.event.Observes;
import javax.inject.Inject;

import org.Macro.BasicProductionManager;
import org.TRX.bwapi.Facade.GameListener;
import org.TRX.bwapi.Facade.Events.OnFrameEvent;

@Singleton
public class DecisionMaker {

	@Inject
	GameListener gm;

	@Inject
	BasicProductionManager basicProductionManager;

	public DecisionMaker() {
		System.out.println("this is the decisionmaker");
		System.out.println(gm.toString());
	}

	public void run() {
		this.gm.run();
	}

	public void onFrame(@Observes OnFrameEvent onFrameEvent) {
		System.out.println("onFrame");
	}

}
